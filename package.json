{
  "name": "api_rest",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx --watch --env-file=.env src/server.ts",
    "migration:show": "npm run typeorm -- migration:show -- -d ./src/database/connection.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:create": "npx typeorm migration:create ./src/config/migrations/",
    "migration:run": "npm run typeorm -- migration:run -- -d ./src/database/connection.ts",
    "migration:revert": "npm run typeorm -- migration:revert -- -d ./src/database/connection.ts"
  },
  "author": "Alex Maia",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.21",
    "@types/validator": "^13.15.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "bcrypt-ts": "^7.0.0",
    "body-parser": "^2.2.0",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0"
  }
}
